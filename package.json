{
  "name": "use-vue-form-validation",
  "type": "module",
  "version": "0.3.0",
  "packageManager": "pnpm@9.9.0",
  "author": "JB Aubr√©e <jb.aubree@protonmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/jbaubree/use-vue-form-validation#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbaubree/use-vue-form-validation.git"
  },
  "bugs": {
    "url": "https://github.com/jbaubree/use-vue-form-validation/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
